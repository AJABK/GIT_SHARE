/**
 * 
 */
package Test;

/**
 * @author AjayBaban
 *
 */
public class test {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		int r = 0;
		System.out.println("i\tj\tk\tr\n");
		for (int i = 1 ; i < 1 ; i++)
		{
			for (int j = 1; j < i + 1; j++)
			{
				for (int k = 1; k < i + j + 1; k++)
				{
					r++;
					System.out.println(i +"\t"+ j + "\t" + k +"\t" + r +"\n");
				}
			}
		}

		System.out.println(r);
	}

}
